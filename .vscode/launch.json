{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Debug (Launch)-TtsMonitorApplication",
      "request": "launch",
      "mainClass": "com.tts.monitor.TtsMonitorApplication",
      "vmArgs": "-Dfile.encoding=UTF-8",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "java",
      "name": "Debug (Attach)",
      "request": "attach",
      "hostName": "localhost",
      "port": 8000
    },
    {
      "type": "node",
      "request": "launch",
      "name": "调试 tiktok-api.js",
      "program": "${workspaceFolder}/scripts/tiktok-api.js",
      "console": "integratedTerminal",
      "outputCapture": "std"
    }
  ]
}